{
  "name": "@honed/:package_name",
  "private": false,
  "version": "0.0.1",
  "license": "MIT",
  "description": ":package_description",
  "keywords": [
    "honed",
    "vue",
    "inertia",
    ":package_name"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run format && npm run type-check && vite build",
    "format": "npx @biomejs/biome format --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "patch": "npm version patch --no-git-tag-version",
    "minor": "npm version minor --no-git-tag-version",
    "major": "npm version major --no-git-tag-version",
    "release": "npm run build && npm publish --access public",
    "init": "npx node ./init.js && npm i"
  },
  "author": ":package_author",
  "repository": {
    "type": "git",
    "url": "https://github.com/honedlabs/:package_name.git"
  },
  "homepage": "https://github.com/honedlabs/:package_name#readme",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "peerDependencies": {
    "vue": "^3.x.0",
    "@inertiajs/vue3": "^1.2.0||^2.0.0",
    "axios": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.3",
    "@vue/test-utils": "^2.4.6",
    "axios": "^1.2.0",
    "jsdom": "^25.0.1",
    "typescript": "~5.4.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  }
}
